  });
            </script>
            <style>
                .slider .indicators .indicator-item {
                    display: inline-block;
                    position: relative;
                    cursor: pointer;
                    height: 16px;
                    width: 16px;
                    margin: 0 12px;
                    background-color: rgb(92, 91, 91);
                    -webkit-transition: background-color .3s;
                    transition: background-color .3s;
                    border-radius: 50%;
                }

                .slider .indicators .indicator-item.active {
                    background-color: rgb(184, 182, 182);
                }
            </style>
            <div class="slider" style="color: white">
                <ul class="slides" style="border-radius: 10px">
                    <% if(!news[0]) { %>
                    <li>
                        <div class="caption center-align">
                            <img src="https://raw.githubusercontent.com/LiquidBlast/discordbotindex-branding/master/logos/logo.png" style="margin: 0px; border-radius: 50%; height: 110px; width: 110px; margin-top: -30px">
